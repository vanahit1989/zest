  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "transform": {
    "\\.js$": "./node_modules/babel-jest"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "stylelint",
      "eslint --fix",
      "git add"
    ]
  },
  "author": "ivan.kopenkov@gmail.com"
