{
  "name": "ant-customization-example",
  "version": "1.0.0",
  "main": "src/index.tsx",
  "scripts": {
    "start": "nodemon --exec \"npm run webpack-dev-server\"",
    "analyze": "npm run webpack -- --config=./webpack/webpack.bundleAnalyze.ts",
    "webpack-dev-server": "npm run ts-commonjs -- webpack-dev-server --progress --config=./webpack/webpack.config.dev.ts",
    "webpack": "npm run ts-commonjs -- webpack --progress",
    "ts-commonjs": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}'",
    "test": "jest --config=jest.config.js --watch",
    "test:watch": "jest --config=jest.config.js --watchAll",
    "lint": "TIMING=1 eslint . --ext .ts,.tsx,.js",
    "lint:css": "stylelint './**/*.tsx'",
    "lint:fix": "npm run lint -- --fix"
  },
  "nodemonConfig": {
    "watch": [
      "tsconfig.json",
      "babel.config.js",
      "src/components/antd/index.ts",
      "webpack/webpack.config.dev.ts",
      "webpack/webpack.config.base.ts"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/react-redux": "^7.1.22",
    "antd": "^4.18.4",
    "class-transformer": "^0.2.3",
    "core-js": "^3.2.1",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "regenerator-runtime": "^0.13.3",
    "styled-components": "^4.3.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.2",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/core-js": "^2.5.2",
    "@types/eslint": "^6.1.1",
    "@types/html-webpack-plugin": "^3.2.1",
